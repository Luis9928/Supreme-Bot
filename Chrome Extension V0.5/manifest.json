  {
    "name": "El Bean Bot",
    "version": "0.6",
    "description": "Bot",
	"background":{
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon" : "images/icon.png",
		"default_title" : "El Bean Bot"
	},
	"permissions": [
		"tabs",
		"activeTab",
    "storage"
	],
	"content_scripts": [
	{
		"matches": ["*://www.supremenewyork.com/*"],
		"js":[ "jquery-3.3.1.min.js",
            "SupremePage/script.js",
            "SupremePage/checkout.js"]
	}
	],
  "icons":{
    "16" : "images/icon.png",
    "128" : "images/icon3.png",
    "48" : "images/icon2.png"
  },
    "manifest_version": 2
  }
